<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abhinavsanghi.vijayasangeetstudent">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.abhinavsanghi.vijayasangeetstudent.about_app"></activity>
        <activity android:name="com.abhinavsanghi.vijayasangeetstudent.profile" />
        <activity
            android:name="com.abhinavsanghi.vijayasangeetstudent.splashscreen"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abhinavsanghi.vijayasangeetstudent.viewattendancepage" />
        <activity
            android:name="com.abhinavsanghi.vijayasangeetstudent.studentdashboard"
            android:label="Dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.abhinavsanghi.vijayasangeetstudent.MainActivity" />
    </application>

</manifest>